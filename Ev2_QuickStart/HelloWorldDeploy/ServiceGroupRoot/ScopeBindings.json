{
  "$schema": "https://ev2schema.azure.net/schemas/2020-01-01/scopeBindings.json",
  "contentVersion": "0.0.0.1",

  "scopeBindings": [
  {
    "scopeTagName": "MyAppCompute",
    "bindings": [
    {
      "find": "_SAMPLE_SKU_",
      "replaceWith": "Free"
    }]
  },
  {
    "scopeTagName": "EastUS2EUAPRegion",
    "bindings": [
    {
      "find": "_WEBAPP_NAME_",
      "replaceWith": "Ev2HelloWorld"
    },
    {
      "find": "_LOCATION_ABBR_",
      "replaceWith": "EastUS2EUAP"
    },
    {
      "find": "_LOCATION_",
      "replaceWith": "East US 2 EUAP"
    },
    {
      "find": "_3MINS_WAIT_TIME_",
      "replaceWith": "PT3M"
    }]
  }]
}
